      "settings": { "foreground": "#56D0DD" }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": { "foreground": "#D8195F" }
    },
    {
      "name": "Language Variable",
      "scope": ["variable.language"],
      "settings": { "foreground": "#D8195F", "fontStyle": "italic" }
    },
    {
      "name": "Variable Name Entity",
