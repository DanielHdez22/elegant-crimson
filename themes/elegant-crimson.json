      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#FF2B59" }
    },
    {
      "name": "Quote Markup",
      "scope": "markup.quote",
      "settings": { "foreground": "#BD52A2F6", "fontStyle": "italic" }
    },
    {
      "name": "Bold Markup",
      "scope": "markup.bold",
      "settings": { "foreground": "#FF2B59", "fontStyle": "bold" }
    },
    {
      "name": "Italic Markup",
      "scope": "markup.italic",
      "settings": { "foreground": "#7FB4FF", "fontStyle": "italic" }
