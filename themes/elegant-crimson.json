  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": { "foreground": "#7A8A9C", "fontStyle": "italic" }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": { "foreground": "#FF2B59", "fontStyle": "bold" }
    },
    {
      "name": "Operator Keyword",
      "scope": "keyword.operator",
      "settings": { "foreground": "#FF7A95" }
    },
    {
      "name": "New/Expression/Logical/Delete Operator Keywords",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": { "foreground": "#FF2B59" }
    },
    {
      "name": "Unit Other Keyword",
      "scope": "keyword.other.unit",
      "settings": { "foreground": "#FF8FB8F6" }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": { "foreground": "#FF2B59", "fontStyle": "bold" }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": { "foreground": "#FF8FB8F6" }
    },
