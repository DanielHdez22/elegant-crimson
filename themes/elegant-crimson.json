    {
      "name": "Type Name Entity & CSharp Storage Type",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": { "foreground": "#EEAF27EA" }
    },
    {
      "name": "Inherited Class Entity",
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#EEAF27EA" }
    },
    {
      "name": "Other Attribute Name Entity",
