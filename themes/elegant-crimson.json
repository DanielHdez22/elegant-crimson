    {
      "name": "Regexp String",
      "scope": "string.regexp",
      "settings": { "foreground": "#FF89A1F4" }
    },
    {
      "name": "Link Other String",
      "scope": "string.other.link",
      "settings": { "foreground": "#7FB4FF" }
