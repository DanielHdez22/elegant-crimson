    },
    {      
      "name": "Class Support",
      "scope": "support.class",
      "settings": { "foreground": "#EEAF27EA" }
    },
    {
      "name": "Type Support",
      "scope": "support.type",
      "settings": { "foreground": "#7FB4FF" }
    },
    {
      "name": "Function Support",
      "scope": "support.function",
      "settings": { "foreground": "#7FB4FF" }
    },
    {
      "name": "Variable Support",
      "scope": "support.variable",
      "settings": { "foreground": "#FF2B59" }
    },
    {
      "name": "Property Name Type Support & Object Literal Key Meta",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": { "foreground": "#9FCBFF" }
